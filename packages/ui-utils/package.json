{
  "name": "@peculiar/ui-utils",
  "version": "0.0.0",
  "private": false,
  "description": "UI utilities",
  "author": "PeculiarVentures Team",
  "homepage": "https://github.com/PeculiarVentures/react-components#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeculiarVentures/react-components.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "esnext": "dist/esnext/index.js",
  "typings": "dist/types/index.d.ts",
  "bugs": {
    "url": "https://github.com/PeculiarVentures/react-components/issues"
  },
  "scripts": {
    "compile": "npm-run-all clean compile:*",
    "compile:esm": "tsc -p .",
    "compile:cjs": "tsc -p . -m commonjs --outDir dist/cjs",
    "compile:esnext": "tsc -p . -t esnext --outDir dist/esnext",
    "clean": "rimraf dist",
    "test:karma": "karma start",
    "test:karma:debug": "karma start --single-run=false --reporters=mocha --debug"
  },
  "dependencies": {
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@peculiar/karma-scripts": "^0.0.0",
    "@types/chai": "^4.2.11",
    "chai": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "files": [
    "dist/"
  ]
}
