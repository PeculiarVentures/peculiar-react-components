{
  "name": "@peculiar/react-components",
  "version": "0.0.0",
  "private": false,
  "description": "React components & styles",
  "keywords": [
    "react",
    "components",
    "styles",
    "theme",
    "ui"
  ],
  "author": "PeculiarVentures Team",
  "homepage": "https://github.com/PeculiarVentures/react-components#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeculiarVentures/react-components.git"
  },
  "scripts": {
    "compile": "npm-run-all clean compile:*",
    "compile:esm": "tsc -p .",
    "compile:cjs": "tsc -p . -m commonjs --outDir dist/cjs",
    "compile:esnext": "tsc -p . -t esnext --outDir dist/esnext",
    "compile:css": "node ./scripts/stylesCompile.js",
    "clean": "rimraf css && rimraf dist",
    "storybook": "start-storybook -p 3000 -s ./css",
    "dev": "npm run compile:css && npm run storybook"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "esnext": "dist/esnext/index.js",
  "typings": "dist/types/index.d.ts",
  "style": "css/index.css",
  "bugs": {
    "url": "https://github.com/PeculiarVentures/react-components/issues"
  },
  "devDependencies": {
    "@peculiar/ui-theme": "file:../ui-theme",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-info": "^5.3.14",
    "@storybook/addon-knobs": "^5.3.14",
    "@storybook/addon-storysource": "^5.3.14",
    "@storybook/react": "^5.3.14",
    "@storybook/source-loader": "^5.3.14",
    "@types/classnames": "^2.2.9",
    "@types/react": "^16.9.22",
    "@types/storybook__addon-info": "^5.2.1",
    "@types/storybook__react": "^5.2.1",
    "awesome-typescript-loader": "^5.2.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.12.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "tslib": "^1.11.0"
  },
  "peerDependencies": {
    "react": "^16.12.0"
  },
  "files": [
    "dist/",
    "css/"
  ]
}
