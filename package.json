{
  "name": "lib-react-components",
  "version": "0.3.6",
  "description": "React components",
  "keywords": [
    "react",
    "react-components",
    "components",
    "ui",
    "javascript"
  ],
  "main": "commonjs/index.js",
  "module": "esm/index.js",
  "sideEffects": false,
  "scripts": {
    "test": "BABEL_ENV=test NODE_ENV=test jest",
    "lint": "eslint --ext .js,.jsx lib app test",
    "development": "cd ./app && npm run development",
    "predevelopment": "npm run theme:create",
    "theme:create": "node lib/utils/build_theme.js",
    "check:types": "tsc --noEmit",
    "build:commonjs": "BABEL_ENV=commonjs babel lib --out-dir commonjs --extensions \".js,.jsx,.ts,.tsx\" --ignore *.css,*.sss,*.spec.js,*.spec.jsx --source-maps",
    "build:esm": "BABEL_ENV=esm babel lib --out-dir esm --extensions \".js,.jsx,.ts,.tsx\" --ignore *.css,*.sss,*.spec.js,*.spec.jsx --source-maps",
    "build": "npm run check:types && npm run build:commonjs && npm run build:esm",
    "prebuild": "npm run clean",
    "clean": "rimraf commonjs && rimraf esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeculiarVentures/react-components.git"
  },
  "author": "PeculiarVentures.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PeculiarVentures/react-components/issues"
  },
  "homepage": "https://PeculiarVentures.github.io/react-components/",
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "hammerjs": "^2.0.8"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "cross-env": "^5.2.0",
    "hammerjs": "^2.0.8",
    "lib-theme-contructor": "0.0.5",
    "prismjs": "^1.14.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-popper": "^1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.15",
    "@types/hammerjs": "^2.0.36",
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.18",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.6.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.27.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "ignore-styles": "^5.0.1",
    "jest": "^23.6.0",
    "jsdom": "^11.6.2",
    "jsdom-global": "^3.0.2",
    "rimraf": "^2.6.2",
    "traverse": "^0.6.6",
    "typescript": "^3.2.2"
  },
  "files": [
    "LICENSE.md",
    "README.md",
    "lib/",
    "commonjs/",
    "esm/",
    "index.d.ts"
  ]
}
