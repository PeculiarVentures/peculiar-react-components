{
  "name": "lib-react-components",
  "version": "0.3.6",
  "description": "React components",
  "keywords": [
    "react",
    "react-components",
    "components",
    "ui",
    "javascript"
  ],
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "typings": "esm/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "start": "cd ./app && npm start",
    "test": "jest --coverage",
    "lint": "eslint --ext .js,.jsx app scripts && tslint --format stylish lib/**/**/*.{ts,tsx}",
    "clean": "rimraf commonjs && rimraf esm",
    "prebuild": "npm run clean",
    "build": "npm run build:esm && npm run build:cjs && npm run build:theme",
    "build:esm": "tsc",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:theme": "node ./scripts/build_theme.js",
    "storybook": "start-storybook -p 3000 -s ./lib/themes"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeculiarVentures/react-components.git"
  },
  "author": "PeculiarVentures.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PeculiarVentures/react-components/issues"
  },
  "homepage": "https://PeculiarVentures.github.io/react-components/",
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lib-theme-contructor": "0.0.5",
    "prismjs": "^1.14.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-popper": "^1.0.2",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@storybook/addon-info": "^4.1.4",
    "@storybook/addon-knobs": "^4.1.4",
    "@storybook/addon-options": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^23.3.10",
    "@types/prismjs": "^1.9.0",
    "@types/react": "^16.7.18",
    "@types/storybook__addon-info": "^3.4.2",
    "@types/storybook__addon-knobs": "^4.0.0",
    "@types/storybook__react": "^4.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.27.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "ignore-styles": "^5.0.1",
    "jest": "^23.6.0",
    "jsdom": "^11.6.2",
    "jsdom-global": "^3.0.2",
    "react-docgen-typescript-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "traverse": "^0.6.6",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  },
  "files": [
    "LICENSE.md",
    "README.md",
    "lib/",
    "cjs/",
    "esm/"
  ]
}
